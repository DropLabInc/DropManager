# DropManager Environment Variables
# Copy this file to .env and fill in your values

# Server Configuration
PORT=8080
NODE_ENV=development

# Google Gemini AI (optional - fallback extraction used if not provided)
GEMINI_API_KEY=your_gemini_api_key_here

# Webhook Security Tokens
INBOUND_TOKEN=your_secure_inbound_webhook_token
CHAT_VERIFICATION_TOKEN=your_google_chat_verification_token

# Optional: Custom header name for inbound webhook token
INBOUND_HEADER_NAME=X-Webhook-Token

# Optional: Cron job token for scheduled tasks
CRON_TOKEN=your_cron_job_token
